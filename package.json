  "version": "0.11.2",
  "homepage": "http://github.com/Strider-CD/strider-simple-worker",
  "main": "./index.js",
  "dependencies": {
    "async": "~0.1.22",
    "lodash": "~1.3.1",
    "eventemitter2": "~0.4.13",
    "strider-runner-core": "~0.1.2",
    "strider-extension-loader": "~0.4.0",
    "mkdirp": "~0.3.5",
    "dirkeeper": "~0.1.0"
  },
  "devDependencies": {
    "mocha": "~1.6.0",
    "chai": "~1.7.2",
    "jshint": "~2.1.9",
    "expect.js": "~0.2.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  },
